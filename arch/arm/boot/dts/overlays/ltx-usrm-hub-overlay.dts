// Definitions for USRM-HUB sonars hub from Luftronix.

/dts-v1/;
/plugin/;

/ {
	compatible = "brcm,bcm2835", "brcm,bcm2708", "brcm,bcm2709";

	fragment@0 {
		target = <&i2c_arm>;
		__overlay__ {
			#address-cells = <1>;
			#size-cells = <0>;
			status = "okay";

			usrm_hub: usrm_hub@63 {
				compatible = "luftronix,usrm_hub";
				reg = <0x63>;
			};
		};
	};
	
	__overrides__ {
		addr = <&usrm_hub>,"reg:0";
	};
};

